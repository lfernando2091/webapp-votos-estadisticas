<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
 <view-state id="listado">
 	<on-entry>
	 	<evaluate expression="DominiosModelosC.nivelesReporte()" result="viewScope.modeloNiveles"/>
	 	<evaluate expression="DominiosModelosC.programasEdoMex()" result="viewScope.modeloProgramas"/>		
	 	<evaluate expression="EncuestaEdomex2017V2ReporteRangoEdadC.modelo(viewScope.modeloNiveles.seleccionado, viewScope.modeloProgramas.seleccionado)" result="viewScope.modeloEdades" />	
	 	<evaluate expression="EncuestaEdomex2017V2ReporteRangoEdadC.subtotal(viewScope.modeloEdades.listado)" result="viewScope.modeloSubtotal" />
	</on-entry>	
	<transition on="actualizar">
			<evaluate expression="EncuestaEdomex2017V2ReporteRangoEdadC.modelo(viewScope.modeloNiveles.seleccionado, viewScope.modeloProgramas.seleccionado)" result="viewScope.modeloEdades" />
			<evaluate expression="EncuestaEdomex2017V2ReporteRangoEdadC.subtotal(viewScope.modeloEdades.listado)" result="viewScope.modeloSubtotal" />	
	</transition>
	
	
 </view-state>
	
</flow>