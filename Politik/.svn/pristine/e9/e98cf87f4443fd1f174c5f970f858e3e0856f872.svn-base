<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
		
		<view-state id="listado">
			<on-entry>
				<evaluate expression="FuerzaCiudadanaResultadoEncuestasCompleto.modeloNiveles()" result="viewScope.modeloNiveles"/>
				<evaluate expression="FuerzaCiudadanaResultadoEncuestasCompleto.modeloRegiones()" result="viewScope.modeloRegiones"/>
				<evaluate expression="FuerzaCiudadanaResultadoEncuestasCompleto.modeloVertical(viewScope.modeloNiveles.seleccionado, viewScope.modeloRegiones.seleccionado)" result="viewScope.modelo"/>
			</on-entry>
			<transition on="actualizar">
				<evaluate expression="FuerzaCiudadanaResultadoEncuestasCompleto.modeloVertical(viewScope.modeloNiveles.seleccionado, viewScope.modeloRegiones.seleccionado)" result="viewScope.modelo"/>
			</transition>
		</view-state>
</flow>