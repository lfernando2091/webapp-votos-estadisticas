package com.saganet.politik.dominios_dm;

public enum EmpresaDO {
	SIN_EMPRESA(0,"Sin Empresa"),
	SAGA(1,"Saganet");
	
	private final Integer id;
	private final String nombre;
	
	private EmpresaDO(Integer id, String nombre){
		this.id = id;
		this.nombre = nombre;
	}

	public Integer getId() {
		return id;
	}

	public String getNombre() {
		return nombre;
	}
	
}
