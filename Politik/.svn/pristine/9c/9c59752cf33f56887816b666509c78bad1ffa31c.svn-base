	<select id="resultadoSeccionPrograma" resultMap="resultadoSeccionPrograma" parameterType="hashmap">
		SELECT  
			row_number() OVER (ORDER BY ee.entidad) AS id,  
			ee.entidad, ee.region_edomex_2017 region, ee.nombre_region, ee.municipio, ee.nombre_municipio, ee.seccion,			
			SUM(CASE WHEN ee.programa_general='GEM' AND r.estatus='EXITOSO' THEN 1 ELSE 0 END) exitoso_gem,
			SUM(CASE WHEN ee.programa_general='GEM' AND r.estatus='NO_HUBO_QUIEN_CONTESTARA' THEN 1 ELSE 0 END) no_hubo_quien_contestara_gem,
			SUM(CASE WHEN ee.programa_general='GEM' AND r.estatus='MENOR_DE_EDAD' THEN 1 ELSE 0 END) menor_de_edad_gem,
			SUM(CASE WHEN ee.programa_general='GEM' AND r.estatus='DOMICILIO_NO_ENCONTRADO' THEN 1 ELSE 0 END) domicilio_no_encontrado_gem,
			SUM(CASE WHEN ee.programa_general='GEM' AND r.estatus='NO_QUISIERON_CONTESTAR' THEN 1 ELSE 0 END) no_quisieron_contestar_gem,
			SUM(CASE WHEN ee.programa_general='GEM' AND r.estatus='OTRO' THEN 1 ELSE 0 END) otro_gem,
			SUM(CASE WHEN ee.programa_general='GEM' THEN 1 ELSE 0 END) totalGem,
			ems.contactos_gem totalTotalGem,
			ems.meta_gem metaGem,			
			SUM(CASE WHEN ee.programa_general='LICONSA' AND r.estatus='EXITOSO' THEN 1 ELSE 0 END) exitoso_liconsa,
			SUM(CASE WHEN ee.programa_general='LICONSA' AND r.estatus='NO_HUBO_QUIEN_CONTESTARA' THEN 1 ELSE 0 END) no_hubo_quien_contestara_liconsa,
			SUM(CASE WHEN ee.programa_general='LICONSA' AND r.estatus='MENOR_DE_EDAD' THEN 1 ELSE 0 END) menor_de_edad_liconsa,
			SUM(CASE WHEN ee.programa_general='LICONSA' AND r.estatus='DOMICILIO_NO_ENCONTRADO' THEN 1 ELSE 0 END) domicilio_no_encontrado_liconsa,
			SUM(CASE WHEN ee.programa_general='LICONSA' AND r.estatus='NO_QUISIERON_CONTESTAR' THEN 1 ELSE 0 END) no_quisieron_contestar_liconsa,
			SUM(CASE WHEN ee.programa_general='LICONSA' AND r.estatus='OTRO' THEN 1 ELSE 0 END) otro_liconsa,
			SUM(CASE WHEN ee.programa_general='LICONSA' THEN 1 ELSE 0 END) totalLiconsa,
			ems.contactos_liconsa totalTotalLiconsa,
			ems.meta_liconsa metaLiconsa,			
			SUM(CASE WHEN ee.programa_general='PROSPERA' AND r.estatus='EXITOSO' THEN 1 ELSE 0 END) exitoso_prospera,
			SUM(CASE WHEN ee.programa_general='PROSPERA' AND r.estatus='NO_HUBO_QUIEN_CONTESTARA' THEN 1 ELSE 0 END) no_hubo_quien_contestara_prospera,
			SUM(CASE WHEN ee.programa_general='PROSPERA' AND r.estatus='MENOR_DE_EDAD' THEN 1 ELSE 0 END) menor_de_edad_prospera,
			SUM(CASE WHEN ee.programa_general='PROSPERA' AND r.estatus='DOMICILIO_NO_ENCONTRADO' THEN 1 ELSE 0 END) domicilio_no_encontrado_prospera,
			SUM(CASE WHEN ee.programa_general='PROSPERA' AND r.estatus='NO_QUISIERON_CONTESTAR' THEN 1 ELSE 0 END) no_quisieron_contestar_prospera,
			SUM(CASE WHEN ee.programa_general='PROSPERA' AND r.estatus='OTRO' THEN 1 ELSE 0 END) otro_prospera,
			SUM(CASE WHEN ee.programa_general='PROSPERA' THEN 1 ELSE 0 END) totalProspera,
			ems.contactos_prospera totalTotalProspera,
			ems.meta_prospera metaProspera,			
			SUM(CASE WHEN r.estatus='EXITOSO' THEN 1 ELSE 0 END) exitoso,
			SUM(CASE WHEN r.estatus='NO_HUBO_QUIEN_CONTESTARA' THEN 1 ELSE 0 END) no_hubo_quien_contestara,
			SUM(CASE WHEN r.estatus='MENOR_DE_EDAD' THEN 1 ELSE 0 END) menor_de_edad,
			SUM(CASE WHEN r.estatus='DOMICILIO_NO_ENCONTRADO' THEN 1 ELSE 0 END) domicilio_no_encontrado,
			SUM(CASE WHEN r.estatus='NO_QUISIERON_CONTESTAR' THEN 1 ELSE 0 END) no_quisieron_contestar,
			SUM(CASE WHEN r.estatus='OTRO' THEN 1 ELSE 0 END) otro,
			COUNT(ee.id_interno) total,
			(ems.contactos_prospera+contactos_liconsa+contactos_gem) totalTotal,
			(ems.meta_prospera+ems.meta_gem+ems.meta_liconsa) meta			
		FROM (SELECT id_contacto,estatus FROM encuestas_resultados.e_edomex UNION SELECT id_contacto,estatus FROM encuestas_resultados.e_edomex_prospera) r			
		JOIN encuestas.encuesta_edomex ee ON (ee.id_interno=r.id_contacto::INTEGER)
		JOIN encuestas_resultados.edomex_millon_secciones ems ON (ems.llave_seccion=(ee.entidad||'-'||ee.seccion))
		<where>
			<if test="municipios!=null">
				ee.municipio IN 
				<foreach collection="municipios" open="(" separator="," close=")" item="item" index="index">
						#{item.idMunicipio}
				</foreach> <if test="programas!=null"> AND</if>
			</if>
			<if test="regiones!=null">
				ee.region_edomex_2017 IN 
				<foreach collection="regiones" open="(" separator="," close=")" item="item" index="index">
						#{item.idParticion}
				</foreach> <if test="programas!=null"> AND</if>
			</if>
			<if test="programas!=null">
				ee.programa_general IN 
				<foreach collection="programas" open="(" separator="," close=")" item="item" index="index">
						#{item}
				</foreach>
			</if>
		</where>
		GROUP BY ee.entidad, ee.region_edomex_2017, ee.nombre_region, ee.municipio, ee.nombre_municipio, ee.seccion, ems.contactos_gem, ems.meta_gem , ems.contactos_liconsa, ems.meta_liconsa , ems.contactos_prospera, ems.meta_prospera 
		ORDER BY ee.entidad, ee.region_edomex_2017, ee.nombre_region, ee.municipio, ee.nombre_municipio, ee.seccion, ems.contactos_gem, ems.meta_gem , ems.contactos_liconsa, ems.meta_liconsa , ems.contactos_prospera, ems.meta_prospera 
	</select>
	
