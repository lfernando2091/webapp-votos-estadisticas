		if(variable.getDfederal()){
			mapa.put("tabla", "dfederales");
			sqlSession.update("catalogos.camposTabla.agregarCampo", mapa);
			
			log.setDescripcion("Se Agrega Campo a Tabla de Propagacion DFederales");
