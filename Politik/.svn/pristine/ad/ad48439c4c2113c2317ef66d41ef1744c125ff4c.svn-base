package com.saganet.politik.dominios;

public enum NivelReporteCompletoDO {
	GENERAL("General", "Estatal"),
	ESTATAL("Estatal", "Regiones"),
	REGION("Región", "Región - Municipios"),
	MUNICIPIO("Municipio", "Municipio - Secciones"),
	SECCION("Sección", "Sección - Manzanas");
	
	private final String nombre;
	private final String nombre2;
	
	private NivelReporteCompletoDO(String nombre, String nombre2){
		this.nombre = nombre;
		this.nombre2 = nombre2;
	}
	
	public String getNombre(){
		return nombre;
	}
	
	public String getNombre2(){
		return nombre2;
	}
}
