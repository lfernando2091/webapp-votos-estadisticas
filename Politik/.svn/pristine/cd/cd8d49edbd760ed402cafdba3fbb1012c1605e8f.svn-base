	<select id="listado" resultMap="entrevistaEntrevistador">
		SELECT 
			Edomex2017EntrevistaEntrevistadorEO.pkey						id, 
			Edomex2017EntrevistaEntrevistadorEO.id_visitador				folio, 
			Edomex2017EntrevistaEntrevistadorEO.p_1							_1, 
