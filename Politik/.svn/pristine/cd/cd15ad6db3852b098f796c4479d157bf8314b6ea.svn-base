package com.saganet.politik.database;

import java.io.Serializable;

import com.saganet.politik.dominios.TiposPoligonoDO;


public class PoligonoData implements Serializable {
	private static final long serialVersionUID = 3377468163330645757L;

	private TiposPoligonoDO tipo;
	private String llave;

	public PoligonoData(TiposPoligonoDO tipo, String llave) {
		super();
		this.tipo = tipo;
		this.llave = llave;
	}

	@Override
	public String toString() {
		return "PoligonoData [tipo=" + tipo + ", llave=" + llave + "]";
	}

	public TiposPoligonoDO getTipo() {
		return tipo;
	}

	public void setTipo(TiposPoligonoDO tipo) {
		this.tipo = tipo;
	}

	public String getLlave() {
		return llave;
	}

	public void setLlave(String llave) {
		this.llave = llave;
	}

}