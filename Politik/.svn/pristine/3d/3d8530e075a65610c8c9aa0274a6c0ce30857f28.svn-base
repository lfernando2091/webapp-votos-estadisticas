<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="administracion.usuariosProgramas">
	
	<select id="usuarioPrograma" parameterType="String" resultType="String">
		SELECT programa
  		FROM administracion.usuarios_programas
  		where nick = '${value}'
	</select>	
</mapper>