<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<view-state id="listado">
		<on-entry>
			<evaluate expression="CatalogosEntidadesC.modelo()" result="viewScope.modeloEntidades"/>
			<evaluate expression="CatalogosDFederalesC.modelo(viewScope.modeloEntidades.seleccionado)" result="viewScope.modeloDFederales"/>
		</on-entry>
		
		<transition on="actualizarDatos">
			<evaluate expression="CatalogosDFederalesC.modelo(viewScope.modeloEntidades.seleccionado)" result="viewScope.modeloDFederales"/>
		</transition>
	</view-state>

</flow>