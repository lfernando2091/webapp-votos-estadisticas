<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<view-state id="filtro">
	<on-entry>
	<evaluate expression="DominiosModelosC.nivelesReporte()" result="flowScope.modeloNiveles"/>
	<evaluate expression="DominiosModelosC.programasEdoMex()" result="flowScope.modeloProgramas"/>
	<evaluate expression="DiaDReportesMetasSeccionalesC.modelo(flowRequestContext, flowScope.modeloNiveles.seleccionado, flowScope.modeloProgramas.seleccionado)"/>
	</on-entry>
	<transition on="actualizar">
	<evaluate expression="DiaDReportesMetasSeccionalesC.modelo(flowRequestContext, flowScope.modeloNiveles.seleccionado, flowScope.modeloProgramas.seleccionado)"/>
	</transition>
	
	</view-state>

</flow>