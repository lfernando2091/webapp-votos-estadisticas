package com.saganet.politik.dominios_dm;

public enum CatMedioDO {
	TV(1, "T.V."),
	RADIO(2, "Radio"),
	PERIODICOS(3, "Periódicos"),
	FACEBOOK(4, "Facebook"),
	TWITTER(5, "Twitter"),
	REVISTAS(6, "Revistas"),
	VECINOS(5, "Vecinos"),
	AMIGOS(6, "Amigos"),
	FAMILIARES(7, "Familiares"),
	NO_CONTESTA(8, "No contesta");

	private final String nombre;
	private final Integer id;
	
	private CatMedioDO(Integer id, String nombre) {
		this.nombre = nombre;
		this.id = id;
	}

	public String getNombre() {
		return nombre;
	}

	public Integer getId() {
		return id;
	}
}
