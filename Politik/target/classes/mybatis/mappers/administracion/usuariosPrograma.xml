<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="administracion.usuariosProgramas">
	<select id="usuarioPrograma" parameterType="String" resultType="com.saganet.politik.dominios.ProgramasEdoMexDO">
		SELECT programa FROM administracion.usuarios_programas WHERE nick = #{value}
	</select>	

	<select id="usuarioPrograma2" parameterType="String" resultType="com.saganet.politik.dominios.ProgramaFuerzaCiudadanaDO">
		SELECT programa FROM administracion.usuarios_programas2 WHERE nick = #{value}
	</select>	
</mapper>